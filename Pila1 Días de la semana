#Ejemplo de Pila - LIFO
Pila=[]
#Incluir elementos - push - apilar
Pila.append("Lunes")
Pila.append("Martes")
Pila.append("Miercoles")
Pila.append("Jueves")
Pila.append("Viernes")
Pila.append("Sabado")
Pila.append("Domingo")
print(Pila)
#Sacar elementos - pop - desapilar
x=Pila.pop()
print(Pila)#lo restante
x=Pila.pop()
print(Pila)
x=Pila.pop()
print(Pila)
x=Pila.pop()
print(Pila)
x=Pila.pop()
print(Pila)
x=Pila.pop()
print(Pila)
