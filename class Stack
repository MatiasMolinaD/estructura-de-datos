class Stack:
    def __init__(self):
        self.items = []
        
    def isEmpty(self):
        return self.items == []
    
    def push(self, item):
        self.items.append(item) 
    
    def pop(self):
        return self.items.pop()  

    def top(self):
        return self.items[len(self.items)-1]
    
    def size(self):
        return len(self.items)
    
s=Stack()
num=int(input("Ingrese el numero de datos:"))
for i in range(num):
    print (s.isEmpty())
    y=int(input("Ingrese un valor: "))
    s.push(y)
